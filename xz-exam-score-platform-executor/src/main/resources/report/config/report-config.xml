<?xml version="1.0" encoding="UTF-8" ?>
<report-config>

    <report-set id="default" base="com.xz.scorep.executor.exportexcel.impl">

        <report-category name="总体成绩分析" range="province">
            <report name="分数分析" class=".total.TotalBasicDataReport"/>

            <report-category name="{{name}}成绩分析" range="school">
                <report name="分数分析" class=".total.TotalBasicDataReport"/>

                <report-category name="{{name}}成绩分析" range="class">
                    <report name="分数分析" class=".total.TotalBasicDataReport"/>
                </report-category>
            </report-category>
        </report-category>
    </report-set>
</report-config>